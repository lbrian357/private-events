<h1>Create a new Event</h1>

<%= form_for @event do |f| %>
  <%= f.label :event_name %>
  <%= f.text_field :name%>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :date_and_time %>
  <%= f.text_field :time %>

  <%= f.submit 'Create Event' %>
<% end %>


